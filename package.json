    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@fyclabs/tools-fyc-react": "^0.0.3",
    "bootstrap": "^5.2.3",
    "bootstrap-sass": "^3.4.1",
    "firebase": "^11.2.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.2",
    "react-select": "^5.10.0",
    "sass": "1.35.1",
    "vite-plugin-svgr": "^4.2.0",
    "yalc": "^1.0.0-pre.53"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
