{
  "name": "template-2024",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "clean": "rm -rf node_modules && rm yarn.lock && yarn install",
    "clean-cache": "rm -rf node_modules/.vite",
    "dev-restart": "yalc update && yarn clean-cache",
    "watch": "nodemon --watch .yalc --exec 'yarn dev-restart'"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
